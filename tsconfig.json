{
  "extends": "./.nuxt/tsconfig.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.vue",
    ".nuxt/types/**/*.d.ts",
    "server/**/*.ts",
    "types/**/*.d.ts"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#app": [".nuxt/types/app"],
      "#build/*": [".nuxt/*"],
      "#types/*": [".nuxt/types/*"],
      "#pages/*": ["src/pages/*"],
      "#components/*": ["src/components/*"],
      "~/*": ["./*"],
      "@/*": ["./src/*"],
      "#composables/*": ["src/composables/*"],
      "#utils/*": ["src/utils/*"],
      "#services/*": ["src/services/*"]
    },
    "types": [
      "@nuxt/schema",
      "@nuxt/types",
      "@sidebase/nuxt-auth"
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "noImplicitAny": false
  }
}